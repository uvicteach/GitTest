*.asset					-text merge=unityyamlmerge linguist-generated
*.brush					-text merge=unityyamlmerge linguist-generated
*.controller			-text merge=unityyamlmerge linguist-generated
*.flare					-text merge=unityyamlmerge linguist-generated
*.fontsettings			-text merge=unityyamlmerge linguist-generated
*.giparams				-text merge=unityyamlmerge linguist-generated
